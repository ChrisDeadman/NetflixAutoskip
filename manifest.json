{
  "name": "Netflix Autoskip",
  "description": "auto-clicks skip buttons",
  "version": "1.0",
  "author": "ChrisDeadman",
  "manifest_version": 2,
  "content_scripts": [{
    "js": ["autoskip.js"],
    "matches": ["https://www.netflix.com/*"]
  }],
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  }
}
